{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Debug file as Admin",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true,
      "args": ["runas", "/user:Administrator", "python", "${file}"]
    }
  ]
}
